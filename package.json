{
  "name": "reactjs-starterkit",
  "version": "1.0.0",
  "description": "A React.js boilerplate with Webpack and react-router",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf ./public",
    "dev:watch": "webpack -d --watch --colors --config webpack.config.dev.js",
    "dev:build": "webpack -d --colors --config webpack.config.dev.js",
    "dev:deploy": "node webpack.server.js",
    "prod:build": "webpack -p --progress --colors --config webpack.config.prod.js",
    "prod:server": "http-server ./",
    "prod:deploy": "concurrent \"npm run prod:build\" \"npm run prod:server\"",
    "start": "npm run dev:deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ayoubdev/reactjs-starterkit.git"
  },
  "bugs": {
    "url": "https://github.com/ayoubdev/reactjs-starterkit/issues"
  },
  "keywords": [
    "reactjs",
    "webpack",
    "react-router",
    "boilerplate",
    "starter",
    "kit",
    "starterkit",
    "starter-kit"
  ],
  "author": "Ayoub ADIB <ayoub.development@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "concurrently": "^2.0.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "http-server": "^0.9.0",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  }
}
