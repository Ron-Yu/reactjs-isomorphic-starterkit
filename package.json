{
  "name": "reactjs-starterkit",
  "version": "1.0.0",
  "description": "A React.js isomorphic template with Webpack and react-router",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "npm run prod:deploy",
    "clean": "node ./bin/clean.js",
    "server:clientside": "node ./bin/server/clientSideRendering.js",
    "server:serverside": "node ./bin/server/serverSideRendering.js",
    "dev:watch:webpack": "webpack -d --watch --colors --config ./config/webpack.clientside.dev.js",
    "dev:build": "webpack -d --colors --config ./config/webpack.clientside.dev.js",
    "dev:watch": "node ./bin/server/hotReload.js",
    "prod:build:clientside": "webpack -p --progress --colors --config ./config/webpack.clientside.prod.js",
    "prod:build:serverside": "webpack -p --progress --colors --config ./config/webpack.serverside.js",
    "prod:deploy:clientside": "npm run prod:build:clientside && npm run server:clientside",
    "prod:deploy:serverside": "npm run prod:build:serverside && npm run server:serverside",
    "prod:deploy": "npm run prod:deploy:serverside"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ayoubdev/reactjs-starterkit.git"
  },
  "bugs": {
    "url": "https://github.com/ayoubdev/reactjs-starterkit/issues"
  },
  "keywords": [
    "reactjs",
    "webpack",
    "react-router",
    "boilerplate",
    "starter",
    "isomorphic",
    "kit",
    "starterkit",
    "starter-kit"
  ],
  "author": "Ayoub ADIB <ayoub.development@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "del": "^2.2.0",
    "eslint": "^2.5.3",
    "eslint-plugin-react": "^4.2.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "fs": "0.0.2",
    "node-sass": "^3.4.2",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "compression": "^1.6.1",
    "express": "^4.13.4",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  }
}
